{
    "version": "2.7.1",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "removeComments": true,
        "lib": ["dom","es2017"],
        "noLib": false,
        "preserveConstEnums": true,
        "declaration": true,
        "sourceMap": true,
        "suppressImplicitAnyIndexErrors": true,
        "outDir": "./js_snippets/",
        "rootDir": "./snippets"
    },
    "exclude": [
        "./js_snippets"
    ]
}
